<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controllers.FloatingController">
    <Button fx:id="back" layoutX="536.0" layoutY="362.0" mnemonicParsing="false" onAction="#backButtonAction"
            text="Back"/>

    <TableView fx:id="table" layoutX="28.0" layoutY="30.0" prefHeight="244.0" prefWidth="259.0">
        <columns>
            <TableColumn fx:id="operator" prefWidth="133.0" text="Floating"/>
            <TableColumn fx:id="time" prefWidth="133.0" text="Time Micro"/>
        </columns>
    </TableView>

    <TableView fx:id="d" layoutX="302.0" layoutY="30.0" prefHeight="244.0" prefWidth="270.0">
        <columns>
            <TableColumn fx:id="operator_d" prefWidth="131.0" text="Double"/>
            <TableColumn fx:id="time_d" prefWidth="138.0" text="Time (Micro)"/>
        </columns>
    </TableView>
    <Button layoutX="146.0" layoutY="284.0" mnemonicParsing="false" onAction="#compute" text="Compute"/>
    <Button  layoutX="398.0" layoutY="284.0" mnemonicParsing="false" onAction="#compute_double"
            text="Compute"/>


</AnchorPane>
